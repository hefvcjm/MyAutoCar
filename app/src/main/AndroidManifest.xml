<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test.myautocar" >

    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_title"
        android:label="@string/str_title"
        android:roundIcon="@drawable/ic_title"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.test.myautocar.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.test.setting.SettingActivity">

        </activity>
    </application>

</manifest>